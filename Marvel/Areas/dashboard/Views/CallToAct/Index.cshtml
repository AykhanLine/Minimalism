@model CallToAct

@if(Model == null)
{
   <a class="btn btn-outline-success btn-sm my-4" href="/dashboard/calltoact/create">Create</a>
}



@if (Model != null)
{
    <table class="table table-striped table-dark">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
            
            </tr>
        </thead>
        <tbody>

            <tr>
                <th scope="row">@Model.Id</th>
                <td>@Model.Name</td>
               
              
                <td>
                    <a href="/dashboard/calltoact/edit/@Model.Id" class="btn btn-outline-warning btn-sm">Edit</a>
                    <a href="/dashboard/calltoact/detail/@Model.Id" class="btn btn-outline-info btn-sm">Detail</a>
                    <a href="/dashboard/calltoact/delete/@Model.Id" class="btn btn-outline-danger btn-sm">Delete</a>
                </td>
            </tr>


        </tbody>
    </table>
}

